tribble(
  ~shard, ~type, ~n,
  glue( "All\n (", Seasonal.LineUp.DT[server!="asia" & !is.na(deck_3),.N],"/2024)"), "'Other' deck >= 1", other1 %>% NROW(),
  glue( "All\n (", Seasonal.LineUp.DT[server!="asia" & !is.na(deck_3),.N],"/2024)"), "'Other' deck >= 2", other2 %>% NROW(),
